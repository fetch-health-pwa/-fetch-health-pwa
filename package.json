{
  "name": "real-estate-crm",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.15.2",
    "@mui/lab": "5.0.0-alpha.139",
    "@mui/material": "5.11.0",
    "@mui/system": "5.11.0",
    "dayjs": "1.11.5",
    "history": "5.3.0",
    "path-to-regexp": "6.2.0",
    "react": "18.2.0",
    "react-declarative": "2.5.373",
    "react-dom": "18.2.0",
    "tss-react": "4.3.4",
    "uuid": "9.0.0"
  },
  "scripts": {
    "start": "cross-env CC_NODE_ENV=development vite --host 0.0.0.0 --port 3000",
    "cleanup": "npx -y rimraf ./node_modules/.vite",
    "start:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "build": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build",
    "tscheck": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "appwrite:start": "docker-compose up",
    "appwrite:fetch": "sh ./scripts/appwrite_fetch.sh",
    "appwrite:push": "sh ./scripts/appwrite_push.sh",
    "translit": "node ./scripts/translit",
    "test:e2e": "npx playwright test --config=playwright.config.ts",
    "test:e2e:debug": "npx playwright test --debug --config=playwright.config.ts",
    "test:e2e:ui": "npx playwright test --ui --config=playwright.config.ts"
  },
  "devDependencies": {
    "@types/node": "16.11.51",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "8.3.4",
    "@vitejs/plugin-legacy": "4.1.1",
    "@vitejs/plugin-react": "4.0.4",
    "@vitejs/plugin-react-swc": "3.3.2",
    "eslint": "8.45.0",
    "terser": "5.19.4",
    "ts-loader": "9.4.2",
    "typescript": "5.0.2",
    "vite": "4.4.5",
    "vite-plugin-environment": "1.1.3",
    "vite-plugin-full-reload": "1.0.5",
    "vite-plugin-node-polyfills": "0.15.0"
  }
}
